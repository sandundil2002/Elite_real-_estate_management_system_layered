<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.elite.controller.RentPropertyFormController">
   <children>
      <AnchorPane layoutX="10.0" layoutY="10.0" prefHeight="400.0" prefWidth="600.0" stylesheets="@../style/style.css">
         <children>
            <ImageView fitHeight="331.0" fitWidth="361.0" layoutX="121.0" layoutY="68.0" opacity="0.37" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../asstets/back.png" />
               </image>
            </ImageView>
            <Label layoutX="185.0" layoutY="8.0" prefHeight="35.0" prefWidth="224.0" text="Property Rent Form" textFill="#ff760f">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Label>
            <Button layoutX="136.0" layoutY="350.0" mnemonicParsing="false" onAction="#btnRentOnAction" prefHeight="28.0" prefWidth="117.0" styleClass="btn" text="Rent">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button layoutX="328.0" layoutY="349.0" mnemonicParsing="false" onAction="#btnRentClearOnAction" prefHeight="28.0" prefWidth="117.0" styleClass="btn" text="Clear">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <TextField fx:id="txtCustomerName" layoutX="328.0" layoutY="203.0" prefHeight="28.0" prefWidth="212.0" promptText="Cutomer Name" styleClass="lablebtn" />
            <TextField fx:id="txtPropertyPrice" layoutX="328.0" layoutY="135.0" prefHeight="28.0" prefWidth="212.0" promptText="Price" styleClass="lablebtn" />
            <DatePicker fx:id="txtDate" layoutX="30.0" layoutY="259.0" prefHeight="26.0" prefWidth="212.0" promptText="Date" />
            <TextField fx:id="txtRentid" editable="false" layoutX="30.0" layoutY="71.0" prefHeight="28.0" prefWidth="212.0" promptText="Rent Id" styleClass="lablebtn" />
            <JFXComboBox fx:id="comProid" layoutX="31.0" layoutY="131.0" prefHeight="26.0" prefWidth="212.0" promptText="Property Id" />
            <JFXComboBox fx:id="comCusid" layoutX="30.0" layoutY="196.0" prefHeight="26.0" prefWidth="212.0" promptText="Customer Id" />
            <TextField fx:id="txtpayId" editable="false" layoutX="326.0" layoutY="71.0" prefHeight="28.0" prefWidth="212.0" promptText="Payment Id" styleClass="lablebtn" />
            <ChoiceBox fx:id="cmdPaymethod" layoutX="184.0" layoutY="306.0" prefHeight="26.0" prefWidth="202.0" />
            <ChoiceBox fx:id="cmdDuration" layoutX="330.0" layoutY="258.0" prefHeight="26.0" prefWidth="212.0" />
         </children>
      </AnchorPane>
   </children>
